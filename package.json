{
  "name": "electron-scrubber",
  "productName": "Scrubber",
  "version": "1.0.0",
  "description": "Electron scrubber",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "compile-win": "electron-packager ./ Scrubber --platform=win32 --arch=x64 --out ./Scrubber --overwrite",
    "compile-mac": "electron-packager ./ Scrubber --platform=darwin --arch=x64 --out ./Scrubber --overwrite",
    "build": "webpack --progress --profile --colors --display-error-details --display-cached",
    "watch": "webpack --watch --progress --profile --colors --display-error-details --display-cached"
  },
  "repository": "https://github.com/nick-bogdanov/electron-parser",
  "keywords": [
    "Electron",
    "parser",
    "scrubber",
    "web scrub"
  ],
  "author": "nickolas.bog@gmail.com",
  "dependencies": {
    "@angular/animations": "^4.2.2",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.3",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/material": "^2.0.0-beta.6",
    "@angular/platform-browser": "^4.0.3",
    "@angular/platform-browser-dynamic": "^4.1.3",
    "@angular/router": "^4.0.0",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "electron-log": "^2.2.6",
    "es6-shim": "^0.33.3",
    "fs-extra": "^3.0.1",
    "json2xls": "^0.1.2",
    "lodash": "^4.17.4",
    "reflect-metadata": "^0.1.2",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.1.1",
    "@types/core-js": "^0.9.41",
    "@types/node": "~6.0.60",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@types/reflect-metadata": "0.0.5",
    "angular2-template-loader": "^0.6.2",
    "electron": "~1.6.2",
    "es6-shim": "^0.34.0",
    "gulp": "^3.9.0",
    "raw-loader": "^0.5.1",
    "ts-loader": "^2.1.0",
    "typescript": "^2.0.3",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
